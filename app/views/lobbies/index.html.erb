<h1>Listing lobbies</h1>

<table>
  <tr>
    <th>Status</th>
    <th>Data</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lobbies.each do |lobby| %>
  <tr>
    <td><%= lobby.status %></td>
    <td><%= lobby.data %></td>
    <td><%= link_to 'Show', lobby %></td>
    <td><%= link_to 'Edit', edit_lobby_path(lobby) %></td>
    <td><%= link_to 'Destroy', lobby, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Lobby', new_lobby_path %>
