<h1>Listing users</h1>

<table>
  <tr>
    <th>Id str</th>
    <th>Screen name</th>
    <th>Status</th>
    <th>Rank</th>
    <th>Point</th>
    <th>Data</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.id_str %></td>
    <td><%= user.screen_name %></td>
    <td><%= user.status %></td>
    <td><%= user.rank %></td>
    <td><%= user.point %></td>
    <td><%= user.data %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
